{# Manage sekoliko user #}
{% extends 'admin/_base_admin.html.twig' %}

{% set menu_administration =true %}
{% set menu_user =true %}

{% block content %}
    <div class="row">
        <div class="col-md-12">
            <div class="bgc-white bd bdrs-3 p-20 mB-20"><h4 class="c-grey-900 mB-20">Administration manage</h4>
                {{ form_start(form) }}
                <div class="row">
                    <div class="col-md-6">
                        {{ form_widget(form.user) }}
                    </div>
                    <div class="col-md-6">
                        {{ form_widget(form) }}
                    </div>
                </div>
                <div class="col-md-12 d-block">
                    <a href="{{ path('administrator_list') }}" class="btn btn-warning">Annuler</a>
                    <input type="submit" class="btn btn-primary float-right" value="Enregistrer">
                </div>
                {{ form_end(form) }}
            </div>
        </div>
    </div>
{% endblock %}